{% if "exposed_php_var_dump" in data.contains_type %}
    <li>{% trans %}The following addresses contain PHP data dumps, e.g. generated by the var_dump function:{% endtrans %}
        <ul>
            {% for report in data.reports %}
                {% if report.report_type == "exposed_php_var_dump" %}
                    <li>
                        {{ report.target }}
                        {{ report_meta(report) }}
                    </li>
                {% endif %}
            {% endfor %}
        </ul>
        <p>
            {% trans %}Please verify whether such data should be publicly available.{% endtrans %}
        </p>
    </li>
{% endif %}
